<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="FreeBoardIcon">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:src">@drawable/baseline_remove_red_eye_black_18dp</item>
    </style>
    <style name="FreeBoardCnt">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text"></item>
        <item name="android:gravity">center|start</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="reviewListFooterIcon">
        <item name="android:layout_height">14dp</item>
        <item name="android:layout_width">14dp</item>
    </style>

    <style name="reviewListFooterText">
        <item name="android:textSize">10sp</item>
    </style>

</resources>