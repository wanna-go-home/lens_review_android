<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.mypage.myarticle.MyArticleActivity">

    <com.wannagohome.viewty.ui.LensCustomTitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:leftImg="@drawable/ic_arrow_to_left"
        custom:titleName="@string/mypage_my_article" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/myArticleListRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>