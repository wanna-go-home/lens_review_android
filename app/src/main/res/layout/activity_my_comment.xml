<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.mypage.myarticlecomment.MyCommentActivity">

    <com.wannagohome.lens_review_android.ui.LensCustomTitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:leftImg="@drawable/ic_arrow_to_left"
        custom:titleName="@string/mypage_my_comments" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/myCommentRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>